/* components/header/header.wxss */
.header{
    width: 100%;
    height: 90rpx;
    background-color: red;
    text-align: center;
    line-height: 90rpx;
    display: flex;
}
.header>view{
    width: 33.33%;
    height: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.header .header_left{ 
    display: flex;
    align-items: center;
    justify-content: center;
}
.header .header_left .icon{
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
}
.header .header_left.back{
    padding-left: 35rpx;
    box-sizing: border-box;
    justify-content: flex-start;
}
.header .header_left>text{
    width: 140rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.header .header_left.back>text{
    width: 84rpx;
}